{
	"name": "vouch-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"deploy:production": "wrangler deploy --env production",
		"types": "pnpm types:cf && pnpm types:db",
		"types:cf": "wrangler types && prettier --write worker-configuration.d.ts",
		"types:db": "supabase gen types typescript --project-id \"jdgcsdazjypgabvkiwky\" > src/types/database.types.ts && prettier --write src/types/database.types.ts",
		"lint": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"check": "pnpm type-check && pnpm lint && pnpm format:check",
		"fix": "pnpm lint:fix && pnpm format",
		"prepare": "pnpm types",
		"sync-kv": "export $(cat .dev.vars | xargs) && pnpm tsx scripts/sync-local-kv.ts"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.81.1",
		"p-all": "^5.0.1",
		"p-retry": "^7.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-perfectionist": "^4.15.1",
		"jiti": "^2.6.1",
		"prettier": "^3.6.2",
		"supabase": "^2.58.5",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"wrangler": "^4.47.0"
	}
}
